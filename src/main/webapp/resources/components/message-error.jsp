<link rel="stylesheet"
	href="${pageContext.request.contextPath}/resources/css/messageStyle.css">
	<div class="blur-background" id="blur-background"></div>
	<div id="popup-error" class="popup">
		<div class="icon-message">
			<i class="far fa-times-circle regular-icon icon-message-image" style="color: #D22B2B; font-size: 100px"></i>
		</div>
		<div class="popup-content">
			<span id="popup-message-error" class="popup-message"></span>
		</div>
		<div class="close">
			<button id="close-popup-error" class="close-popup">Close</button>
		</div>
	</div>
